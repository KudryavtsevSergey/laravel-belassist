{
  "name": "sun/belassist",
  "description": "Bel Assist Service",
  "authors": [
    {
      "name": "Kudryavtsev Sergey",
      "email": "invaderman@mail.ru"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "ext-json": "*",
    "ext-simplexml": "*",
    "symfony/serializer": "^6.1",
    "symfony/property-access": "^6.1",
    "phpdocumentor/reflection-docblock": "^5.3",
    "guzzlehttp/guzzle": "^7.5"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Sun\\BelAssist\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sun\\BelAssist\\BelAssistServiceProvider"
      ],
      "aliases": {
        "BelAssist": "Sun\\BelAssist\\Facade"
      }
    }
  }
}
